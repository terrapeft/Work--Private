                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT V.[aID]
      ,V.[aUID]
      , UCS.UserContactAddressId
      ,V.[uUserName]
