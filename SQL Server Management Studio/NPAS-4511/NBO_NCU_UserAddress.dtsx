        ,CASE WHEN V. [aTitle] = 'Not Selected' THEN '' ELSE  [aTitle]          END as [aTitle]
      ,V.[aCompany]
      ,V.[aAddress1]
      ,V.[aAddress2]
      ,V.[aAddress3]
      ,V.[aCity]
      ,V.[aCounty]
      ,case WHEN LTRIM(RTRIM(V.[aState])) = 'Not Applicable' THEN '' ELSE V.aState END as aState
      ,V.[aPostCode]
      ,dbo.[fn_NBO_NCU_Country_Mapping](V.[aCID]) as acid 
      ,V.[aTel]
      ,V.[aFax]
      ,V.[aMobTel]
      ,V.[Ext]
      ,V.[InternationalDiallingCodeId]
      ,V.[aCreationDate]
      ,V.[aCreatedBy]
      ,V.[creatorUserId]
      ,V.[CreatorCultUserId]
      ,V.[ApplicationId]
      ,V.[ReasonId]
      ,V.[aUpdatedDate]
