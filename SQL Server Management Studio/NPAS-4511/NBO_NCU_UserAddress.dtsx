      ,V.[UserTypeId]
      ,V.[aUpdatedBy]
      ,V.[aActive]
      ,V.[aDefault]
      ,V.[aSessionId]
      ,V.[aUpdateReason]
  FROM [dbo].[VW_NBOAddresses]  V
JOIN Interim.[UserContactAddress_Stage] UCS ON
UCS.UserId = V.auid AND UCS.AddressId = V.aId AND
UCS.ParentUserContactAddressId IS NOT NULL
WHERE   V.auid = ? and V.aId = ?</property>
