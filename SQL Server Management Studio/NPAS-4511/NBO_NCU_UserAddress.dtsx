                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT x.UserId, x.ueromoneyPhone, x.ueromoneyFax, x.ueromoneyemail, x.uThirdParty
FROM
(SELECT USERID,[1] as ueromoneyPhone,[2] as ueromoneyFax,[3]  as ueromoneyemail,[4]  as uThirdParty
FROM 
(select userid, UsercontactpermissiontypeId, CASE (SubscriptionAllowed | EditorialAllowed | MarketingAllowed) 
WHEN 0 THEN 0
ELSE 1
END AS Value
FROM 
Customer.UserContactPreferences where UsercontactpermissiontypeId in (1,2,3,4)
AND UserId IN (Select userId from Interim.[ContactPref_Stage]
WHERE ActionCode = 'I')
) up
PIVOT (sum(value) FOR usercontactpermissiontypeId in ([1],[2],[3],[4])) as pvt) X
Where X.UserId = ?</property>
