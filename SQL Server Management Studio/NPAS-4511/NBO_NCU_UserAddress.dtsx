              SQLTask:BypassPrepare="False"
              SQLTask:SqlStatementSource="SET IDENTITY_INSERT Interim.Addresses ON;&#xA;&#xA;INSERT INTO  Interim.Addresses ([aID] ,[aUID],[aAncestorID],[aParentID],[aEclipseCode]&#xA;&#x9;&#x9;&#x9;,[aJobTitle],[aCompany],[aAddress1],[aAddress2],[aAddress3],[aCity],[aCounty]&#xA;      ,[aState],[aPostCode],[aCID],[aTel],[aFax],[aMobTel],[aCreationDate],[aCreatedBy]&#xA;      ,[aUpdatedDate],[aUpdatedBy],[aActive],[aDefault],[aSessionID],[aUpdateReason])&#xA;&#xA;SELECT [aID],[aUID],[aAncestorID],[aParentID],[aEclipseCode],[aJobTitle],[aCompany],[aAddress1]&#xA;      ,[aAddress2],[aAddress3],[aCity],[aCounty],[aState],[aPostCode],[aCID],[aTel],[aFax]&#xA;      ,[aMobTel],[aCreationDate],[aCreatedBy] ,[aUpdatedDate],[aUpdatedBy],[aActive]&#xA;      ,[aDefault],[aSessionId],[aUpdateReason]&#xA;  FROM [Interim].[Addresses_ReplTest] A&#xA;JOIN Interim.[UserContactAddress_Stage] U &#xA;ON A.auid = U.UserId and A.aid = U.AddressId&#xA;Where A.aid = ?;&#xA;&#xA;SET IDENTITY_INSERT Interim.Addresses OFF;&#xA;&#xA;&#xA;&#xA;Set IDENTITY_INSERT INTERIM.UserDetails ON;&#xA;&#xA;&#xA;INSERT INTO INTERIM.UserDetails ([uID] ,[uEclipseCustomerID] ,[uUsername] ,[uPassword]&#xA;      ,[uOldPassword] ,[uEmailAddress],[uTitle],[uForenames],[uSurname],[uCompany]&#xA;      ,[uJobTitle],[uCompanyType] ,[uIndustry],[uRegisteredVia],[uEuromoneyPhone]&#xA;      ,[uEuromoneyFax],[uEuromoneyEmail],[uThirdParty] ,[uDPAConfirmed],[uCreationDate]&#xA;      ,[uCreatedBy],[uUpdateDate],[uUpdatedBy] ,[userid],[uComments] ,[uAreasofInterest]&#xA;      ,[uHtmlEmail],[uUpdateComment],[uQuestion],[uOldusername],[uFreeIssueID],[uTestUser]&#xA;      ,[uHFIApproved] ,[uHFISubscriberID] ,[uDescription])&#xA;SELECT U.[UserId] as [uID]&#xA;&#x9;   ,NULL as [uEclipseCustomerID]&#xA;      ,U.[UserName] as [uUsername]&#xA;      ,U.[EncryptPassword] as [uPassword]&#xA;      , NULL as [uOldPassword]&#xA;      ,[uEmailAddress]&#xA;      ,[aTitle] as [uTitle]&#xA;      ,[uForeNames] &#xA;      ,[uSurname]&#xA;      ,[aCompany] as [uCompany]&#xA;      ,[aJobTitle] as [uJobTitle]&#xA;      , NULL as [uCompanyType]  &#xA;      ,NULL as [uIndustry]   &#xA;      ,CASE US.CreatorUserId &#xA;      WHEN NULL THEN &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(Select ApplicationName FROM Logon.Application &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Where ApplicationId = US.CreatorApplicationId) + ' -- ' + &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(Select UserName FROM logon.Users where UserId = US.CreatorCultUserId) &#xA;      &#xA;      ELSE &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(Select ApplicationName FROM Logon.Application &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;Where ApplicationId = US.CreatorApplicationId) + ' -- ' + &#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;(Select UserName FROM logon.Users where UserId = US.CreatorUserId) &#xA;      END&#xA;      AS [uRegisteredVia]&#xA;      ,0 as [uEuromoneyPhone]&#xA;      ,0 as [uEuromoneyFax]&#xA;      ,0 as [uEuromoneyEmail]&#xA;      ,0 as [uThirdParty]&#xA;      ,'' as [uDPAConfirmed]&#xA;&#x9;  ,[aCreationDate] as [uCreationDate]&#xA;      ,[aCreatedBy] as [uCreatedBy]&#xA;      ,[aUpdatedDate] as [uUpdateDate]&#xA;      ,[aUpdatedBy] as [uUpdatedBy]&#xA;      ,U.[userid] &#xA;      ,'Replication From NBO' as [uComments]&#xA;      ,NULL as [uAreasofInterest]  &#xA;      ,NULL as [uHtmlEmail]  &#xA;      ,[aUpdateReason] as [uUpdateComment] &#xA;      ,NULL as [uQuestion]  &#xA;      ,NULL as [uOldusername]  &#xA;      ,NULL as [uFreeIssueID]&#xA;      ,CASE U.UserTypeId&#xA;      WHEN 2 THEN 1&#xA;      WHEN 4 THEN 1&#xA;      ELSE 0 END&#xA;      AS [uTestUser]&#xA;      ,NULL as [uHFIApproved]  &#xA;      ,NULL as [uHFISubscriberID]   &#xA;      ,NULL as [uDescription]   &#xA;            &#xA;&#x9;FROM Interim.[LogonUsers_Stage]   U&#xA;  JOIN [dbo].[VW_NBOAddresses] V&#xA;  ON V.aUID = U.UserId AND V.aActive = 1&#xA;  JOIN logon.UserSummary US ON Us.UserId = U.UserId&#xA;Where U.UserId = ?;&#xA;&#xA;Set IDENTITY_INSERT INTERIM.UserDetails OFF;" xmlns:SQLTask="www.microsoft.