          SQLTask:SqlStatementSource="USE BackOffice&#xA;&#xA;DECLARE @uid INT&#xA;DECLARE @aid INT&#xA;DECLARE @ucid INT&#xA;DECLARE @cnt INT&#xA;&#xA;IF OBJECT_ID('tempdb..##NoInterim') IS NOT NULL&#xA;    DROP TABLE ##NoInterim&#xA;&#xA;CREATE TABLE ##NoInterim (userid int, addressid int,maxucid int,Interimucid int)&#xA;&#xA;DECLARE db_cursor CURSOR FOR&#xA;select A.addressid , A.UserId from INTERIM.UserContactaddress_Stage A&#xA;where A.PArentUSerContactaddressid IS NULL AND A.addressid !=0&#xA;AND A.USERID NOT IN (5005748 , 5005785)&#xA;UNION&#xA;select B.addressid , B.UserId from INTERIM.UserContactaddress_Stage B&#xA;JOIN Customer.UserContactAddress C WITH(NOLOCK) &#xA;ON C.UserId = B.UserId AND &#xA;C.UserContactAddressId = B.ParentUserContactAddressId AND&#xA;C.AddressId = 0&#xA;where B.USERID NOT IN (5005748 , 5005785) AND B.addressid !=0&#xA;&#xA;OPEN db_cursor   &#xA;FETCH NEXT FROM db_cursor INTO @aid, @uid   &#xA;&#xA;WHILE @@FETCH_STATUS = 0   &#xA;BEGIN   &#xA;&#xA;SET @ucid = 0&#xA;&#xA;&#xA;&#xA;select @ucid = COUNT(*) from Interim.AddressUCAMap where &#xA;usercontactaddressid IN (Select max(usercontactaddressid) from customer.UserContactaddress where &#xA;addressid = @aid and userid = @uid)&#xA;&#xA;IF @ucid = 0 &#xA;BEGIN&#xA;&#xA;DECLARE @mucid INT -- Max ucid&#xA;DECLARE @Iucid INT -- INterim.AddressUCAMAP Ucid&#xA;&#xA;Select @mucid = max(usercontactaddressid) from customer.UserContactaddress where &#xA;addressid = @aid and userid = @uid&#xA;&#xA;SELECT @Iucid = usercontactaddressid FROM Interim.AddressUCAMap Where&#xA;addressid = @aid and userid = @uid&#xA;&#xA;INSERT INTO ##NoInterim VALUES (@uid,@aid,@mucid,@Iucid)&#xA;&#xA;END&#xA;&#xA;FETCH NEXT FROM db_cursor INTO @aid, @uid   &#xA;&#xA;END   &#xA;&#xA;CLOSE db_cursor   &#xA;DEALLOCATE db_cursor&#xA;&#xA;SELECT @cnt = COUNT(*) FROM ##NoInterim&#xA;&#xA;-- Run this script to fix the UCAID in Interim.AddressUCAMAP table&#xA;&#xA;IF @cnt &gt;= 1 &#xA;BEGIN&#xA;&#xA;UPDATE I&#xA;SET usercontactaddressid = N.maxucid&#xA;FROM Interim.AddressUCAMap I JOIN ##NoInterim N &#xA;ON N.addressid = I.addressid AND N.userid = I.userid &#xA;&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
