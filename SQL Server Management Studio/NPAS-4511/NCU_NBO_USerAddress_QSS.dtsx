      ,CASE WHEN a.[aFax] IS NOT NULL AND LEN(RTRIM(LTRIM(a.[aFax]))) &lt; 5 THEN NULL ELSE a.[aFax] END AS aFax
      ,CASE WHEN a.[aMobTel] IS NOT NULL AND LEN(RTRIM(LTRIM(a.[aMobTel]))) &lt; 5 THEN NULL ELSE a.[aMobTel] END AS aMobTel
