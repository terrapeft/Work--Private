	WHEN  a.[aAddress1] IS NULL THEN ':' ELSE a.[aAddress1] END as aAddress1
      ,a.[aAddress2]
      ,a.[aAddress3]
      ,a.[aCity]
      ,a.[aCounty]
      ,a.[aState]
      ,CASE WHEN ISNULL(RTRIM(a.[aPostCode]), '') = '' THEN
