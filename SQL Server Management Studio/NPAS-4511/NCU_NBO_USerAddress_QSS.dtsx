                <component
                  refId="Package\User Update\Update Users\OLE DB Source BackOffice"
                  componentClassID="Microsoft.OLEDBSource"
                  contactInfo="OLE DB Source;Microsoft Corporation;Microsoft SqlServer v9; (C) 2005 Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;7"
                  description="OLE DB Source"
                  name="OLE DB Source BackOffice"
                  usesDispositions="true"
                  version="7">
                  <properties>
                    <property
                      dataType="System.Int32"
                      description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                      name="CommandTimeout">0</property>
                    <property
                      dataType="System.String"
                      description="Specifies the name of the database object used to open a rowset."
                      name="OpenRowset"></property>
                    <property
                      dataType="System.String"
                      description="Specifies the variable that contains the name of the database object used to open a rowset."
                      name="OpenRowsetVariable"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT	
	u.UserId 
	,u.UserName 
	,u.EncryptPassword
	,ISNULL(u.UserTypeId, 0) AS UserTypeId
	,c.initials
	,c.GenderId	
	,Case WHEN upnp.Phone = '' THEN NULL ELSE upnp.Phone END AS Phone
    ,Case WHEN upnp.Fax = '' THEN NULL ELSE upnp.Fax END AS Fax
    ,Case WHEN upnp.Mobile = '' THEN NULL ELSE upnp.Mobile END AS Mobile
	,upnp.Ext
	,upnp.InternationalDiallingCodeId
	,uca.creatorUserId
	,uca.CreatorCultUserId
	,uca.ReasonId
	,uca.UserContactAddressId
FROM
	BackOffice.Logon.Users u WITH(NOLOCK)
	JOIN BackOffice.Logon.UserDefaultCommunication udc	WITH(NOLOCK) ON u.UserId = udc.UserId
	JOIN BackOffice.Customer.ActiveUserContactAddress auca	WITH(NOLOCK) ON udc.ActiveUserContactAddressId = auca.ActiveUserContactAddressId
	JOIN BackOffice.Customer.UserContactAddress uca	WITH(NOLOCK) ON u.UserId = uca.UserId AND auca.UserContactAddressId = uca.UserContactAddressId
	JOIN Customer.Contact c WITH(NOLOCK) ON uca.ContactId = c.ContactId
	LEFT JOIN
	(	SELECT 
			UserContactAddressId
		,MIN([P1])		AS PhoneId
		,MIN([1])		AS Phone
		,MIN([E1])		AS Ext
		,MIN([P2])		AS FaxId
		,MIN([2])		AS Fax
		,MIN([P3])		AS MobileId
		,MIN([3])		AS Mobile
		,MIN(InternationalDiallingCodeId) AS InternationalDiallingCodeId
		FROM
			(
			SELECT
					pn.PhoneNumberId
					,ucapn.UserContactAddressId
					,pnt.PhoneNumberTypeId
					,'P' + CONVERT(VARCHAR(11),pnt.PhoneNumberTypeId)	AS PhoneNumberTypeId2	--	solely here for second pivot
					,'E' + CONVERT(VARCHAR(11),pnt.PhoneNumberTypeId)	AS PhoneNumberTypeId3	--	solely here for third pivot
					,pn.PhoneNumber
					,pne.PhoneNumberExtension
					,pn.InternationalDiallingCodeId
			FROM
				Customer.UserContactAddressPhoneNumber ucapn WITH(NOLOCK)
			INNER JOIN
				Customer.PhoneNumberType pnt WITH(NOLOCK) ON ucapn.PhoneNumberTypeId = pnt.PhoneNumberTypeId
			INNER JOIN
				Customer.PhoneNumber pn WITH(NOLOCK) ON ucapn.PhoneNumberId = pn.PhoneNumberId
			LEFT JOIN
				Customer.PhoneNumberExtension pne WITH(NOLOCK) ON ucapn.PhoneNumberExtensionId = pne.PhoneNumberExtensionId
			) dt
			--	should only every be one set of three whilst we are using current forms of data entry
		PIVOT
			(
			MIN(PhoneNumber)	
			FOR PhoneNumberTypeId IN ([1],[2],[3])
			) pvt1
		PIVOT
			(
			MIN(PhoneNumberId)
			FOR PhoneNumberTypeId2 IN ([P1],[P2],[P3])
			) pvt2
		PIVOT
			(
			MIN(PhoneNumberExtension)
			FOR PhoneNumberTypeId3 IN ([E1])
			) pvt3
		GROUP BY
			UserContactAddressId
	) upnp	ON uca.UserContactAddressId = upnp.UserContactAddressId
WHERE u.UserId = ?</property>
