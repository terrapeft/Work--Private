                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 	
0 AS [sID]        
  ,       1 AS [sUID]
      ,P.NCUPublicationID AS [sPID]
      ,SubscriptionStartDateTime AS [sStartDate]
      ,CASE WHEN SubscriptionTypeId =  3 THEN SubscriptionEndDateTime END AS [sExpiryDate]
      ,CASE WHEN SubscriptionTypeId =  2 THEN SubscriptionEndDateTime END AS [sTrialExpiryDate]
      ,1 AS [sMasterRecord]
      ,1 AS [sAllowedSessions]
      ,1 AS [sNumberOfLogons]
      ,1 AS [sNumberOfGUIDs]
      ,CASE WHEN SubscriptionTypeId = 1 THEN 2 ELSE 10 END AS [sStatus]
      ,SubscriptionStartDateTime AS [sCreationDate]
      ,0 AS [sCreatedBy]
      ,NULL AS [sUpdateDate]
      ,NULL AS [sUpdatedBy]
      ,NULL AS [sFirstLoggedOn]
      ,NULL AS [sComments]
      ,NULL AS [sIPOnly]
      ,NULL AS [sCopyPaste]
      ,CASE WHEN UserGroupTypeId=  2 THEN 1 ELSE 0 END  AS [sIPLogOnCheck]
      ,NULL AS [sTrialTerminatedDate]
,SC.SubscriptionCode AS SubscriptionNumber	
,trGUID as sGUID
FROM [Interim].[UserGroupSubscription_Stage] AS UGS with(nolock)
JOIN Logon.UserGroup  AS UG with(nolock) ON UGS.UserGroupId = UG.UserGroupId
