                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT  
      SU.SubscriptionUserId as [sID]
      ,S.SubscriptionCodeID AS SubscriptionCodeId
      ,SU.UserID AS [sUID]
      ,NCUPublicationID AS [sPID]
      ,SubscriptionStartDateTime AS [sStartDate]
      ,CASE WHEN SubscriptionTypeId =  3 THEN SubscriptionEndDateTime END AS [sExpiryDate]
      ,CASE WHEN SubscriptionTypeId =  2 THEN SubscriptionEndDateTime END AS [sTrialExpiryDate]
      ,1 AS [sMasterRecord]
      ,1 AS [sAllowedSessions]
      ,1 AS [sNumberOfLogons]
      ,1 AS [sNumberOfGUIDs]
      ,CASE WHEN SubscriptionTypeId = 1 THEN 2 ELSE 10 END AS [sStatus]
      ,SubscriptionStartDateTime AS [sCreationDate]
      ,UserId AS [sCreatedBy]
      ,NULL AS [sUpdateDate]
      ,NULL AS [sUpdatedBy]
      ,NULL AS [sFirstLoggedOn]
      ,NULL AS [sComments]
      ,NULL AS [sIPOnly]
      ,NULL AS [sCopyPaste]
      ,NULL AS [sIPLogOnCheck]
      ,NULL AS [sTrialTerminatedDate]
      ,SU.[Action] AS [Action]
,trGUID as [sGUID]
  FROM [Interim].[SubscriptionUser_Stage] AS SU
  JOIN Orders.Subscription AS S ON S.SubscriptionId = SU.SubscriptionId
  JOIN [Product].[Products] AS P ON P.ProductID = S.ProductID
 WHERE [Action] = 'I' AND [Type]='D' AND [trGUID]=?</property>
