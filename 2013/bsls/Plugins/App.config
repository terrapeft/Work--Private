<?xml version="1.0" encoding="utf-8" ?>
<configuration>
   <appSettings/>
   <system.runtime.remoting>
      <application>
         <channels>
            <channel ref="tcp" displayName="TcpChannel" port="0">
               <clientProviders>
                  <formatter ref="binary" typeFilterLevel="Full"  />
               </clientProviders>
               <serverProviders>
                  <formatter ref="binary" typeFilterLevel="Full" />
               </serverProviders>
            </channel>
         </channels>
      </application>
      <debug loadTypes="true" />
   </system.runtime.remoting>
</configuration>